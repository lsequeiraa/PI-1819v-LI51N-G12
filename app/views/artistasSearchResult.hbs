<h4>Artistas:</h4>
<div class="table-responsive">
    <table class="table table-striped table-sm">
        <thead>
        <tr>            
            <th>Name</th>
            <th>mbID</th>
            <th>Listeners</th>
        </tr>
        <body>
            {{#each .}}
                <tr>
                    <td>{{name}}</td>
                    <td><a type="button" class="text-primary" name="button_artist" title="View Top Albums"><u>{{mbid}}</u></a></td>
                    <td>{{listeners}}</td>
                </tr>
            {{/each}}
        </body>
        </thead>
    </table>
</div>